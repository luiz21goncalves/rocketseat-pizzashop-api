{
  "extends": ["@rocketseat/eslint-config/node", "plugin:drizzle/all"],
  "plugins": ["drizzle", "simple-import-sort", "sort-keys"],
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error",
    "sort-keys": "off",
    "sort-keys/sort-keys-fix": "error"
  }
}